# ----------------------------------------------------------------------
# Generate geometry
# ----------------------------------------------------------------------
playback 'geometry.jou'

# ----------------------------------------------------------------------
# Set scheme
# ----------------------------------------------------------------------
volume all scheme tetmesh

# ----------------------------------------------------------------------
# Set discretization size
# ----------------------------------------------------------------------
volume all size 3000

# ----------------------------------------------------------------------
# Generate the mesh
# ----------------------------------------------------------------------
mesh volume all

# ----------------------------------------------------------------------
# Generate blocks and nodesets
# ----------------------------------------------------------------------
playback 'setbc.jou'

# ----------------------------------------------------------------------
# Export exodus file
# ----------------------------------------------------------------------
export mesh "tet4.exo" dimension 3 overwrite

